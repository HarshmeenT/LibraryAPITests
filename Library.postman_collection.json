		{
			"name": "AddBook",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// test cases for Addbook API written in javacript and chai assertions. Postman gives an existing object to work with pm :",
							"",
							"//making a JSON object to work with properties inside.",
							"",
							"const json_data = pm.response.json();",
							"console.log(json_data);",
							"pm.environment.set(\"book_id\",json_data.ID);",
							"",
							"//**TC #1** Verify if API responses returns Success Codes with Proper Assertions",
							"pm.test(\"Validate status response\", function()",
							"{",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"//**TC # 2** Validate if Add Book API sucessfully added the book by verifing the text message is equal to expected message.",
							"pm.test(\"Validate if book was added successfully\", function()",
							"{",
							"//we used expect when euqating two values",
							"    pm.expect(json_data).to.have.property(\"Msg\");",
							"    pm.expect(json_data).to.have.property(\"ID\")",
							"    pm.expect(json_data.Msg).to.eql(\"successfully added\");",
							"});",
							"",
							"//**TC # 3** Verify response time",
							"pm.test(\"Performance : Validate Addbook API response time is under expected limits\", function()",
							"{",
							"    pm.expect(pm.response.responseTime).to.below(1000);",
							"});",
							"",
							"//**TC # 4** Verify response headers",
							"pm.test(\"Validate response headers\", function()",
							"{",
							"    pm.response.to.have.header(\"Content-Type\");",
							"    pm.expect(pm.response.headers.get(\"Content-Type\")).to.eql(\"application/json;charset=UTF-8\");",
							"    pm.response.to.have.header(\"server\");",
							"    //pm.expect(pm.response.headers.get(\"server\")).to.eql(\"Apache/2.4.52 (Ubuntu)\");",
							"});",
							"",
							"",
							"//**TC # 5** Verify ID Validation logic ",
							"",
							"",
							"",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
