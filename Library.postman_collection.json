					"listen": "prerequest",
					"script": {
						"exec": [
							"",
							"//Steps to execute before the tests",
							"const code = pm.globals.get(\"companyCode\");",
							"const val = (pm.variables.replaceIn('{{$randomInt}}') + 1);",
							"",
							"pm.collectionVariables.set(\"isbn\",code+val);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
